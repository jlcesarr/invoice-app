    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.7",
    "husky": "^8.0.2",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
